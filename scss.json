{
	"Bidi Margin Left" : {
		"prefix": "biMarLe",
		"body": "@include bidi-props(margin-left, px, margin-right, unset);"
	},
	"Bidi Margin Right" : {
		"prefix": "biMarRi",
		"body": "@include bidi-props(margin-right, px, margin-left, unset);"
	},
	"Bidi Margin X" : {
		"prefix": "biMarX",
		"body": [
			"@include bidi-values(margin-left, px, px);",
			"@include bidi-values(margin-right, px, px);"
		]
	},
	"Bidi Margin All" : {
		"prefix": "biMarAll",
		"body": [
			"margin-bottom: px;",
			"margin-top: px;",
			"@include bidi-values(margin-left, px, px);",
			"@include bidi-values(margin-right, px, px);"
		]
	},
